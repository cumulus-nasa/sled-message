{
  "workflow_config": {
    "Example": {
      "bar": "{meta.foo}",
      "provider": "{$.meta.provider}",
      "cumulus_message": {
        "outputs": [
          {
            "source": "{{$}}",
            "destination": "{{$.payload}}"
          },
          {
            "source": "{{$.input.anykey}}",
            "destination": "{{$.meta.baz}}"
          }
        ]
      }
    }
  },
  "cumulus_meta": {
    "task": "Example",
    "message_source": "local",
    "id": "id-1234"
  },
  "meta": {
    "foo": "bar",
    "provider": {
      "id": "MODAPS",
      "config": {
          "num_connections": 5
      }
    },
    "baz": "anyvalue"
  },
  "payload": {
    "input": { "anykey": "anyvalue" },
    "config": { 
      "bar": "bar",
      "provider": {
        "id": "MODAPS",
        "config": {
            "num_connections": 5
        }
      }
    }
  },
  "exception": "None"
}
